<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite parallel="tests"    name="Suite">	
<groups>	
	<run>
		<include name="purchase"></include>
	</run>
</groups>
	
  <test thread-count="5" name="Add product to cart">
    <classes>
      <class name="Prakash.tests.AddProductToCart"/>
    </classes>
  </test> <!-- Test -->
  
  <test thread-count="5" name="Error Validation">
    <classes>
      <class name="Prakash.tests.ErrorValidation"/>
    </classes>
  </test> <!-- Test -->
  
</suite> <!-- Suite -->
